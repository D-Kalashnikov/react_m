{
  "parser": "babel-eslint",
  "env": {
	"browser": true,
	"es6": true,
	"jest": true,
	"node": true
  },
  "settings": {
	"import/resolver": {
	  "webpack": {
		"config": "webpack.config"
	  }
	}
  },
  "extends": [
	"prettier",
	"eslint:recommended",
	"plugin:react/recommended",
	"plugin:prettier/recommended"
  ],
  "rules": {
	"jsx-quotes": [
	  1,
	  "prefer-double"
	]
  },
  "plugins": [
	"import",
	"prettier",
	"react"
  ]
}
